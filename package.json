    "start:dist": "npm run dist && node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yceman/minimalist-f1.git"
  },
  "keywords": [
    "Minimal",
    "API",
    "Node",
    "Typescript",
    "Fastify"
  ],
  "author": "Marcos Morais",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yceman/minimalist-f1/issues"
  },
  "homepage": "https://github.com/yceman/minimalist-f1#readme",
  "dependencies": {
    "fastify": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/typescript": "^2.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
