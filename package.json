{
  "name": "minimalist-f1",
  "version": "1.0.0",
  "description": "Minimalist API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dist": "tsup src",
    "start:dev": "tsx --env-file=.env src/server.ts",
